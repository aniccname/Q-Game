{
    "compilerOptions": {
        "jsx": "react",
        "module": "nodenext",
        "target": "esnext",
        "lib": ["esnext", "DOM"],
        "sourceMap": true,
        "strict": true,
        "emitDeclarationOnly": false,
        "noEmit": true,
        "moduleResolution": "NodeNext",
        "esModuleInterop": true,
    },
    "exclude": [
        "node_modules"
    ],
}